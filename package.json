{
  "name": "madcss",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "test": "vitest run",
    "format": "biome format",
    "lint": "biome lint",
    "check": "biome check",
    "deploy": "npm run build && wrangler deploy -c dist/server/wrangler.json",
    "preview": "npm run build && vite preview",
    "cf-typegen": "wrangler types",
    "db:generate": "drizzle-kit generate",
    "db:migrate:local": "wrangler d1 migrations apply mad-css-db --local",
    "db:migrate:prod": "wrangler d1 migrations apply mad-css-db --remote",
    "db:migrate:staging": "wrangler d1 migrations apply mad-css-db-staging --remote",
    "db:studio": "drizzle-kit studio",
    "db:setup": "npm run db:generate && npm run db:migrate:local"
  },
  "dependencies": {
    "@cloudflare/vite-plugin": "^1.20.1",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-devtools": "^0.9.2",
    "@tanstack/react-router": "^1.146.0",
    "@tanstack/react-router-devtools": "^1.146.0",
    "@tanstack/react-router-ssr-query": "^1.146.0",
    "@tanstack/react-start": "^1.146.0",
    "@tanstack/router-plugin": "^1.146.0",
    "@xyflow/react": "^12.10.0",
    "better-auth": "^1.4.10",
    "drizzle-orm": "^0.45.1",
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwindcss": "^4.1.18",
    "vite-tsconfig-paths": "^6.0.3"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@tanstack/devtools-vite": "^0.4.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "better-sqlite3": "^12.5.0",
    "drizzle-kit": "^0.31.8",
    "jsdom": "^27.4.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.16",
    "web-vitals": "^5.1.0",
    "wrangler": "^4.58.0"
  }
